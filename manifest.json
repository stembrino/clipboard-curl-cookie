{
    "manifest_version": 3,
    "name": "Clipboard Curl Cookie",
    "version": "1.0",
    "permissions": [
      "cookies",
      "activeTab"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "favicon.ico",
        "48": "favicon.ico",
        "128": "favicon.ico"
      }
    }
  }